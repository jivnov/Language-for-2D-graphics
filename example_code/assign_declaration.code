#viewport 1000 1000

func Def() {
    rect A [20%];
    rect B [80%];
    A left B;
}

func Abc() {
    rect A [30%, 100%];
    rect B [40%, 100%];
    rect C [30%, 100%];
    A left B;
    B left C;
}

shape DefTest [100%, 20%] = Def();
shape A [100%, 40%] = Abc();
shape B [100%, 30%] = Abc();

DefTest bot B;
A top B;

draw DefTest;